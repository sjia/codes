<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.javatrian.struts2.model.UserInfo" table="user_info">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native"></generator>
        </id>
        <property name="loginName" type="java.lang.String">
            <column name="login_name" length="64" not-null="true" />
        </property>
        <property name="pwd" type="java.lang.String">
            <column name="pwd" length="64" not-null="true" />
        </property>
        <property name="userName" type="java.lang.String">
            <column name="user_name" length="64" not-null="true" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" length="256" />
        </property>
        <property name="test" type="java.lang.String">
            <column name="test" length="64" />
        </property>
    </class>
</hibernate-mapping>
